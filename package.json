{
  "name": "react-fetch-hook",
  "version": "1.9.5",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "React fetch hook",
  "scripts": {
    "flow": "flow",
    "flow:check": "flow check",
    "typescript": "tsc -p tsconfig.json --noEmit",
    "test": "jest --silent",
    "size": "size-limit",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "run:example:basic": "parcel ./examples/basic/index.html",
    "run:example:depends": "parcel ./examples/depends/index.html",
    "run:example:pagination": "parcel ./examples/pagination/index.html --no-cache"
  },
  "eslintConfig": {
    "extends": [
      "lessmess/lib",
      "lessmess/react-hooks"
    ],
    "rules": {
      "func-style": "off"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest/setupTests.js"
  },
  "size-limit": [
    {
      "ignore": [
        "react"
      ],
      "limit": "556 B",
      "path": "index.js",
      "running": false
    }
  ],
  "files": [
    "utils",
    "index.js",
    "index.js.flow",
    "index.d.ts",
    "usePromise.js",
    "usePromise.js.flow",
    "usePromise.d.ts",
    "usePaginatedRequest.js",
    "usePaginatedRequest.js.flow",
    "usePaginatedRequest.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ilyalesik/react-fetch-hook.git"
  },
  "keywords": [
    "react",
    "hooks",
    "fetch"
  ],
  "author": "Ilya Lesik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ilyalesik/react-fetch-hook/issues"
  },
  "homepage": "https://github.com/ilyalesik/react-fetch-hook#readme",
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-react-flow-props-to-prop-types": "^0.15.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-lessmess": "0.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-import-helpers": "^0.1.4",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.89.0",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-fetch-mock": "^2.1.0",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-infinite-scroller": "^1.2.4",
    "react-test-renderer": "^16.8.0",
    "react-testing-library": "^6.0.0",
    "rimraf": "^2.6.2",
    "size-limit": "^1.3.2",
    "typescript": "^3.4.5"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  }
}
